<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.UNICLUB">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="UNICLUB"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.UNICLUB.OptionsActivity"></activity>
        <activity android:name="com.example.UNICLUB.ProfilActivity" />
        <activity android:name="com.example.UNICLUB.YorumlarActivity" />
        <activity android:name="com.example.UNICLUB.GonderiActivity" />
        <activity android:name="com.example.UNICLUB.AnaActivity" />
        <activity android:name="com.example.UNICLUB.KaydolActivity" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity android:name="com.example.UNICLUB.GirisActivity" />
        <activity android:name="com.example.UNICLUB.BaslangicActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>